{
  "expo": {
    "name": "DriveWise",
    "slug": "drivewise-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./mobile/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./mobile/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.drivewise.mobile"
    },
    "android": {
      "package": "com.drivewise.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./mobile/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./mobile/assets/favicon.png"
    }
  }
}
